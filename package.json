{
  "name": "vue3-vant-mobile",
  "type": "module",
  "version": "1.3.0",
  "packageManager": "pnpm@8.13.1",
  "description": "Vue + Vite H5 Starter Template",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env MOCK_SERVER_PORT=8086 vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode=development",
    "preview": "npm run build && vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "@vant/touch-emulator": "^1.4.0",
    "@vant/use": "^1.6.0",
    "axios": "^1.6.2",
    "echarts": "^5.4.3",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "resize-detector": "^0.3.0",
    "store": "^2.0.12",
    "vant": "^4.8.1",
    "vconsole": "^3.15.1",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5",
    "vue-router-better-scroller": "^0.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.4.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@types/nprogress": "^0.2.3",
    "@types/store": "^2.0.5",
    "@unocss/eslint-plugin": "^0.58.0",
    "@unocss/preset-rem-to-px": "^0.58.0",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "cz-emoji-chinese": "^0.3.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "mockjs": "^1.1.0",
    "postcss-mobile-forever": "^4.0.0",
    "rollup": "^4.9.1",
    "rollup-plugin-visualizer": "^5.11.0",
    "terser": "^5.26.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-mock-dev-server": "^1.4.3",
    "vite-plugin-vconsole": "^2.0.1",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.25"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss"
      ],
      "allowedVersions": {
        "rollup": "^4.x"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji-chinese"
    },
    "cz-emoji-chinese": {
      "skipQuestions": [
        "body",
        "scope"
      ]
    }
  }
}
